{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Yhtua",
  "version": "2.3.0",
  "identifier": "com.yhtua.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run generate"
  },
  "app": {
    "windows": [
      {
        "title": "Yhtua",
        "width": 600,
        "height": 800,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "DeveloperTool"
  }
}

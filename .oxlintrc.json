{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["typescript"],
  "categories": {
    "correctness": "error"
  },
  "rules": {
    "eslint/no-unused-vars": "off",
    "typescript/no-non-null-assertion": "off",
    "typescript/no-explicit-any": "off",
    "eslint/prefer-const": "error",
    "eslint/prefer-template": "error"
  }
}
